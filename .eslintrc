{
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "globals": {

    },
    "settings": {
    },
    "plugins": [ "json", "html" ],

    "parser": "babel-eslint",
    "rules": {
        "no-extend-native": 1,
        "no-shadow": [1,{"allow":["data"]}],
        "no-caller": 1,
        "semi": [1, "always"],
        "semi-spacing": [1,{"after":true}],
        "space-before-blocks": [1,"always"],
        "space-before-function-paren": [1,"never"],
        "space-in-parens": [1,"never"],
        "space-infix-ops": [1,{"int32Hint":true}],
        "keyword-spacing": [1,{"before": true, "after": true, "overrides": { "if": { "after": false }, "for": { "after": false }, "while": { "after": false }, "switch": { "after": false }}}],
        "space-unary-ops": [1,{"words":true}],
        "spaced-comment": [1,"always",{}],
        "arrow-parens": [1,"as-needed"],
        "arrow-spacing": [1,{"before":true,"after":true}],
        "constructor-super": 1,
        "no-class-assign": 1,
        "no-const-assign": 1,
        "no-this-before-super": 1,
        "prefer-const": 1,
        "prefer-spread": 1,
        "comma-dangle": [1,"never"],
        "no-cond-assign": [1,"except-parens"],
        "no-constant-condition": 1,
        "no-debugger": 1,
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 1,
        "no-empty-character-class": 1,
        "no-empty": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": [1,"all"],
        "no-extra-semi": 1,
        "no-func-assign": 1,
        "no-inner-declarations": [1,"both"],
        "no-invalid-regexp": 1,
        "no-irregular-whitespace": 1,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 1,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 1,
        "no-unreachable": 1,
        "use-isnan": 1,
        "valid-typeof": 1,
        "no-unexpected-multiline": 1,
        "block-scoped-var": 1,
        "dot-notation": 1,
        "dot-location": [1,"property"],
        "eqeqeq": [1,"smart"],
        "guard-for-in": 0,
        "no-alert": 1,
        "no-extra-bind": 1,
        "no-fallthrough": 1,
        "no-loop-func": 1,
        "no-multi-spaces": 1,
        "no-native-reassign": 1,
        "no-redeclare": 1,
        "no-return-assign": [1,"always"],
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-useless-call": 1,
        "no-warning-comments": [0,{"location":"start"}],
        "vars-on-top": 1,
        "no-shadow-restricted-names": 1,
        "no-undef": 1,
        "no-undefined": 1,
        "no-unused-vars": [1, {"varsIgnorePattern":"Twenty"}],
        "no-use-before-define": 1,
        "array-bracket-spacing": [1,"never",{}],
        "brace-style": [1,"1tbs",{"allowSingleLine":true}],
        "camelcase": [0,{"properties":"always"}],
        "comma-spacing": [1,{"before":false,"after":true}],
        "comma-style": 1,
        "consistent-this": 1,
        "id-length": [1,{"min":2,"max":30,"exceptions": ["i","e","x","y"]}],
        "indent": [1,4,{"SwitchCase": 1}],
        "key-spacing": [1,{"afterColon":true}],
        "lines-around-comment": [1,{"beforeBlockComment":true,"beforeLineComment":true,"allowBlockStart":true,"allowBlockEnd":true}],
        "linebreak-style": [1,"unix"],
        "no-lonely-if": 0,
        "no-mixed-spaces-and-tabs": 1,
        "no-multiple-empty-lines": [1,{"max":1}],
        "no-nested-ternary": 1,
        "no-new-object": 1,
        "no-spaced-func": 1,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 1,
        "curly": [1, "multi-line"],
        "object-curly-spacing": [1,"never",{}],
        "padded-blocks": [1,{ "classes": "always" }],
        "quote-props": [1,"consistent-as-needed"],
        "quotes": [1,"double","avoid-escape"],
        "prefer-arrow-callback": 1,
        "wrap-iife": [1, "outside"]
    }
}
